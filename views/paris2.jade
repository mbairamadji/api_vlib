extends lagny
block content
	#map(style='height: 500px; width: 80%')
		script.
			var map = L.map("map").setView([48.85550131286469, 2.422055733613231], 16);
			L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'}).addTo(map);
			var marker = L.marker([48.85550131286469, 2.422055733613231]).addTo(map);
			marker.bindPopup("<b>Station Vlib 31004</b><br>Paris 2").openPopup();


