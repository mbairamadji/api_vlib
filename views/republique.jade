extends lagny
block content
	#map(style='height: 500px; width: 80%')
		script.
			var map = L.map("map").setView([48.85281419134583, 2.419307088753658], 16);
			L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'}).addTo(map);
			var marker = L.marker([48.85281419134583, 2.419307088753658]).addTo(map);
			marker.bindPopup("<b>Station Vlib 31002</b><br>Republique").openPopup();


