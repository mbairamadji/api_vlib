extends lagny
block content
	#map(style='height: 500px; width: 80%')
		script.
			var map = L.map("map").setView([48.857702134021, 2.43736866129498], 16);
			L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'}).addTo(map);
			var marker = L.marker([48.857702134021, 2.43736866129498]).addTo(map);
			marker.bindPopup("<b>Station Vlib 31008</b><br>Vincennes").openPopup();


